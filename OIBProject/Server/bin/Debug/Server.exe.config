<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="Server.Service">
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:4200"/>
          </baseAddresses>
        </host>
        <endpoint address="StartProcess"
                  binding="netTcpBinding"
                  contract="Server.IStartProcess"/>
        <endpoint address="StopProcess"
                  binding="netTcpBinding"
                  contract="Server.IStopProcess"/>
        <endpoint address="ShowActiveProcesses"
                  binding="netTcpBinding"
                  contract="Server.IShowActiveProcesses"/>
        <endpoint address="StopAllProcess"
                  binding="netTcpBinding"
                  contract="Server.IStopAllProcess"/>
        <endpoint address="ReadLogFile"
                  binding="netTcpBinding"
                  contract="Server.IReadLogFile"/>
      </service>
    </services>
  </system.serviceModel>
</configuration>