<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <system.serviceModel>
    <client>
      <endpoint name="StartProcess"
                address="net.tcp://localhost/StartProcess"
                binding="netTcpBinding"
                contract="Server.IStartProcess"/>
      <endpoint name="StopProcess"
                address="net.tcp://localhost/StopProcess"
                binding="netTcpBinding"
                contract="Server.IStopProcess"/>
      <endpoint name="ShowActiveProcess"
                address="net.tcp://localhost/ShowActiveProcess"
                binding="netTcpBinding"
                contract="Server.IShowActiveProcess"/>
      <endpoint name="StopAllProcesses"
                address="net.tcp://localhost/StopAllProcesses"
                binding="netTcpBinding"
                contract="Server.IStopAllProcesses"/>
      <endpoint name="ReadLogFile"
                address="net.tcp://localhost/ReadLogFile"
                binding="netTcpBinding"
                contract="Server.IReadLogFile"/>
    </client>
  </system.serviceModel>
</configuration>